<skin>

    <!-- JButton styling -->
    <style type="button" id="default">
        <component>
            <focusable>true</focusable>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebButtonPainter">
            <round>3</round>
            <shadeWidth>2</shadeWidth>
            <paintBackground>true</paintBackground>
            <webColoredBackground>true</webColoredBackground>
        </painter>
    </style>

    <!-- WebRootPane styling -->
    <style type="button" id="window-button" extends="default">
        <component>
            <focusable>false</focusable>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebButtonPainter">
            <margin>1,4,1,4</margin>
            <round>6</round>
        </painter>
    </style>
    <style type="button" id="window-minimize-button" extends="window-button">

    </style>
    <style type="button" id="window-maximize-button" extends="window-button">

    </style>
    <style type="button" id="window-close-button" extends="window-button">

    </style>

    <!-- WebColorChooserPanel styling -->
    <style type="button" id="colorchooser-button" extends="default" />
    <style type="button" id="colorchooser-ok" extends="colorchooser-button" />
    <style type="button" id="colorchooser-reset" extends="colorchooser-button" />
    <style type="button" id="colorchooser-cancel" extends="colorchooser-button" />

    <!-- WebSplitPane styling -->
    <style type="button" id="splitpane-onetouch" extends="default">
        <component>
            <focusable>false</focusable>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebButtonPainter">
            <margin>0,0,0,0</margin>
            <undecorated>true</undecorated>
        </painter>
    </style>
    <style type="button" id="splitpane-onetouch-left" extends="splitpane-onetouch" />
    <style type="button" id="splitpane-onetouch-right" extends="splitpane-onetouch" />


</skin>