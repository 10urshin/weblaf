<skin>

    <!-- com.alee.laf.desktoppane.WebInternalFrame -->

    <!-- Internal frame -->
    <style type="internalFrame" id="default">
        <component>
            <opaque>false</opaque>
            <background>90,90,90,220</background>
        </component>
        <painter class="com.alee.managers.style.skin.web.WebInternalFramePainter">
            <round>4</round>
            <webColoredBackground>false</webColoredBackground>
        </painter>
    </style>

    <!-- Internal frame decoration control buttons -->
    <style type="button" id="internalframe" extends="default" padding="4,6,4,6">
        <component>
            <focusable>false</focusable>
        </component>
        <painter class="com.alee.managers.style.skin.web.WebButtonPainter">
            <round>4</round>
            <shadeWidth>0</shadeWidth>
            <innerShadeWidth>2</innerShadeWidth>
            <rolloverDarkBorderOnly>false</rolloverDarkBorderOnly>
        </painter>
    </style>
    <style type="button" id="internalframe-minimize" extends="internalframe" />
    <style type="button" id="internalframe-maximize" extends="internalframe" />
    <style type="button" id="internalframe-close" extends="internalframe" />

</skin>