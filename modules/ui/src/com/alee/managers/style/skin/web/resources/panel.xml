<skin>

    <!-- JPanel styling -->
    <style type="panel" id="default">
        <component>
            <background>237,237,237</background>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <round>2</round>
            <shadeWidth>2</shadeWidth>
            <paintBackground>true</paintBackground>
            <webColoredBackground>true</webColoredBackground>
        </painter>
    </style>
    <style type="panel" id="transparent" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
    </style>
    <style type="panel" id="white" extends="default">
        <component>
            <opaque>true</opaque>
            <background>white</background>
        </component>
    </style>

    <!-- WebPopOver container styling -->
    <style type="panel" id="pop-over" extends="default">
        <component>
            <background>white</background>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPopOverPainter">
            <borderColor>128,128,128,128</borderColor>
            <round>4</round>
            <shadeWidth>20</shadeWidth>
            <shadeTransparency>0.75</shadeTransparency>
            <cornerWidth>10</cornerWidth>
            <transparency>0.95</transparency>
        </painter>
    </style>

    <!-- WebBreadcrumb styling -->
    <style type="panel" id="breadcrumb" extends="default">
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <paintFocus>true</paintFocus>
        </painter>
    </style>
    <style type="panel" id="breadcrumb-element" extends="default">
        <ui>
            <margin>1,5,1,5</margin>
        </ui>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebBreadcrumbPanelPainter">
            <undecorated>false</undecorated>
        </painter>
    </style>

    <!-- WebDocumentPane styling -->
    <style type="panel" id="document-pane" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
    </style>

    <!-- WebFileDrop styling -->
    <style type="panel" id="file-drop" extends="default">
        <component>
            <background>white</background>
            <focusable>true</focusable>
        </component>
        <ui>
            <margin>2,2,2,2</margin>
        </ui>
    </style>

    <!-- WebDockableFrame styling -->
    <style type="panel" id="dockable-frame-title" extends="default">
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <round>0</round>
            <shadeWidth>0</shadeWidth>
            <paintBackground>true</paintBackground>
            <webColoredBackground>true</webColoredBackground>
            <paintTop>false</paintTop>
            <paintLeft>false</paintLeft>
            <paintBottom>true</paintBottom>
            <paintRight>false</paintRight>
        </painter>
    </style>
    <style type="panel" id="dockable-frame-buttons" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
    </style>

    <!-- WebSyntaxLabel styling -->
    <style type="panel" id="syntax-panel" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
        <ui>
            <margin>2,4,2,4</margin>
        </ui>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebSyntaxPanelPainter">
            <style>thick</style>
        </painter>
    </style>

    <!-- Custom editor gutter panel styling -->
    <style type="panel" id="editor-gutter" extends="default">
        <component>
            <background>white</background>
        </component>
    </style>

    <!-- WebComponentPanel styling -->
    <style type="panel" id="components-panel" extends="default">
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <webColoredBackground>false</webColoredBackground>
        </painter>
    </style>

    <!-- WebRootPane styling -->
    <style type="panel" id="window-title-panel" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <margin>4,5,4,10</margin>
        </painter>
    </style>

    <!-- WebColorChooserPanel styling -->
    <style type="panel" id="colorchooser" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <margin>2,5,2,5</margin>
            <webColoredBackground>false</webColoredBackground>
        </painter>
    </style>
    <style type="panel" id="colorchooser-buttons" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <margin>5,0,3,0</margin>
            <webColoredBackground>false</webColoredBackground>
        </painter>
    </style>

</skin>