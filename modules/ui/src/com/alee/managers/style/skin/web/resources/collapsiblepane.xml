<skin>

    <!-- com.alee.extended.panel.WebCollapsiblePane -->

    <!-- Collapsible pane -->
    <style type="panel" id="collapsiblepane" extends="default">
        <component>
            <focusable>true</focusable>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <round>2</round>
            <webColoredBackground>false</webColoredBackground>
            <paintFocus>true</paintFocus>
        </painter>
    </style>

    <!-- Header panel -->
    <style type="panel" id="collapsiblepane-header" extends="default">
        <component>
            <opaque>true</opaque>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <round>2</round>
            <shadeWidth>0</shadeWidth>
        </painter>
    </style>

    <!-- Expand button -->
    <style type="button" id="collapsiblepane-expand" extends="default">
        <component>
            <focusable>false</focusable>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebButtonPainter">
            <undecorated>true</undecorated>
            <moveIconOnPress>false</moveIconOnPress>
        </painter>
    </style>

    <!-- Content panel -->
    <style type="panel" id="collapsiblepane-content" extends="default">
        <component>
            <opaque>false</opaque>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <round>2</round>
            <shadeWidth>0</shadeWidth>
        </painter>
    </style>

</skin>