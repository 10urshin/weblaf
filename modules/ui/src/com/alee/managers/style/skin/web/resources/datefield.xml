<skin>

    <!-- com.alee.extended.date.WebDateField -->

    <!-- Choose button -->
    <style type="button" id="datefield-choose" extends="default">
        <component>
            <focusable>false</focusable>
        </component>
        <painter id="painter" class="com.alee.managers.style.skin.web.WebButtonPainter">
            <round>2</round>
            <shadeWidth>0</shadeWidth>
            <moveIconOnPress>false</moveIconOnPress>
            <rolloverDecoratedOnly>true</rolloverDecoratedOnly>
        </painter>
    </style>

    <!-- Calendar -->
    <style type="panel" id="datefield" extends="calendar">
        <painter id="painter" class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <round>2</round>
            <shadeWidth>0</shadeWidth>
            <paintFocus>false</paintFocus>
        </painter>
    </style>

</skin>