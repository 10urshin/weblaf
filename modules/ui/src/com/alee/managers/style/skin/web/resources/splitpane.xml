<skin>

    <!-- com.alee.laf.splitpane.WebSplitPane -->

    <!-- Split pane -->
    <style type="splitPane" id="default">
        <painter class="com.alee.managers.style.skin.web.WebSplitPanePainter" />
    </style>

    <!-- Split one-touch collapse buttons -->
    <style type="button" id="splitpane-onetouch" extends="default">
        <component>
            <focusable>false</focusable>
        </component>
        <painter class="com.alee.managers.style.skin.web.WebButtonPainter">
            <undecorated>true</undecorated>
        </painter>
    </style>
    <style type="button" id="splitpane-onetouch-left" extends="splitpane-onetouch" />
    <style type="button" id="splitpane-onetouch-right" extends="splitpane-onetouch" />

</skin>