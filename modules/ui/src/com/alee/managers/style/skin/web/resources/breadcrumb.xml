        <painter class="com.alee.managers.style.skin.web.WebPanelPainter">
            <undecorated>false</undecorated>
            <paintFocus>true</paintFocus>
        </painter>
    </style>

    <!-- Label element -->
    <style type="label" id="breadcrumb-element" extends="default" padding="1,5,1,5">
        <painter class="com.alee.managers.style.skin.web.WebBreadcrumbLabelPainter" />
